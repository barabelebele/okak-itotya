                gender = message.text.split()[1].lower()
                if gender in ['male', 'female']:
                    USER_GENDERS[message.from_user.id] = gender
                    await message.reply(f"‚úÖ –ü–æ–ª —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω: {'–º—É–∂—Å–∫–æ–π' if gender == 'male' else '–∂–µ–Ω—Å–∫–∏–π'}")
                else:
                    await message.reply("‚ùå –ò—Å–ø–æ–ª—å–∑—É–π: /set_gender male –∏–ª–∏ /set_gender female")
            except:
                await message.reply("‚ùå –ò—Å–ø–æ–ª—å–∑—É–π: /set_gender male –∏–ª–∏ /set_gender female")
        
        @self.dp.message_handler(commands=['stats'])
        async def show_stats(message: types.Message):
            stats = f"üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞:\n"
            stats += f"–í—Å–µ–≥–æ –≤–æ–ø—Ä–æ—Å–æ–≤: {len(QUESTIONS)}\n"
            stats += f"–û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö: {len(self.sent_questions)}/{len(QUESTIONS)}\n"
            stats += f"–ò–Ω—Ç–µ—Ä–≤–∞–ª: –∫–∞–∂–¥—ã–µ {INTERVAL_HOURS} —á.\n"
            stats += f"\n–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–∏:\n"
            for uid, gender in USER_GENDERS.items():
                stats += f"ID {uid}: {'–º—É–∂—Å–∫–æ–π' if gender == 'male' else '–∂–µ–Ω—Å–∫–∏–π'}\n"
            await message.reply(stats)
        
        @self.dp.message_handler(commands=['ask'])
        async def ask_now(message: types.Message):
            await self.send_question_to_chat()
        
        @self.dp.callback_query_handler(lambda c: c.data and c.data.startswith(('done_', 'fail_')))
        async def process_callback(callback_query: CallbackQuery):
            user_id = callback_query.from_user.id
            gender = USER_GENDERS.get(user_id, "female")  # –ü–æ —É–º–æ–ª—á–∞–Ω–∏—é –∂–µ–Ω—Å–∫–∏–π
            
            if callback_query.data.startswith('done_'):
                if gender == "male":
                    text = "‚úÖ –í—ã–ø–æ–ª–Ω–∏–ª! –ú–æ–ª–æ–¥–µ—Ü! üëç"
                else:
                    text = "‚úÖ –í—ã–ø–æ–ª–Ω–∏–ª–∞! –ú–æ–ª–æ–¥–µ—Ü! üëç"
            else:  # fail_
                if gender == "male":
                    text = "‚ùå –ù–µ –≤—ã–ø–æ–ª–Ω–∏–ª... –í —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–ª—É—á–∏—Ç—Å—è! üí™"
                else:
                    text = "‚ùå –ù–µ –≤—ã–ø–æ–ª–Ω–∏–ª–∞... –í —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑ –ø–æ–ª—É—á–∏—Ç—Å—è! üí™"
            
            await self.bot.answer_callback_query(callback_query.id)
            await self.bot.send_message(
                callback_query.from_user.id,
                text
            )
    
    async def send_question_to_chat(self):
        """–û—Ç–ø—Ä–∞–≤–ª—è–µ—Ç —Å–ª—É—á–∞–π–Ω—ã–π –≤–æ–ø—Ä–æ—Å –≤ —á–∞—Ç"""
        if not QUESTIONS:
            await self.bot.send_message(CHAT_ID, "‚ùå –í–æ–ø—Ä–æ—Å—ã –∑–∞–∫–æ–Ω—á–∏–ª–∏—Å—å!")
            return
        
        # –í—ã–±–∏—Ä–∞–µ–º —Å–ª—É—á–∞–π–Ω—ã–π –≤–æ–ø—Ä–æ—Å
        question = random.choice(QUESTIONS)
        
        # –°–æ–∑–¥–∞–µ–º –∫–ª–∞–≤–∏–∞—Ç—É—Ä—É
        keyboard = InlineKeyboardMarkup(row_width=2)
        keyboard.add(
            InlineKeyboardButton("‚úÖ –í—ã–ø–æ–ª–Ω–µ–Ω–æ", callback_data="done"),
            InlineKeyboardButton("‚ùå –ù–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–æ", callback_data="fail")
        )
        
        # –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
        await self.bot.send_message(
            CHAT_ID,
            f"üéØ *–í–æ–ø—Ä–æ—Å –¥–ª—è –æ–±—â–µ–Ω–∏—è:*\n\n{question}\n\n_–û—Ç–º–µ—Ç—å, –≤—ã–ø–æ–ª–Ω–∏–ª–∞ –ª–∏ —Ç—ã —ç—Ç–æ –∏–ª–∏ –æ—Ç–≤–µ—Ç–∏–ª–∞ –Ω–∞ –≤–æ–ø—Ä–æ—Å:_",
            parse_mode=ParseMode.MARKDOWN,
            reply_markup=keyboard
        )
        
        # –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–Ω—ã–π –≤–æ–ø—Ä–æ—Å
        self.sent_questions[question] = datetime.now()
    
    async def scheduled_questions(self):
        """–ü–ª–∞–Ω–∏—Ä–æ–≤—â–∏–∫ –æ—Ç–ø—Ä–∞–≤–∫–∏ –≤–æ–ø—Ä–æ—Å–æ–≤"""
        while True:
            await self.send_question_to_chat()
            await asyncio.sleep(INTERVAL_HOURS * 3600)  # –ü–µ—Ä–µ–≤–æ–¥–∏–º —á–∞—Å—ã –≤ —Å–µ–∫—É–Ω–¥—ã
    
    def run(self):
        """–ó–∞–ø—É—Å–∫ –±–æ—Ç–∞"""
        # –ó–∞–ø—É—Å–∫–∞–µ–º –ø–ª–∞–Ω–∏—Ä–æ–≤—â–∏–∫ –≤ —Ñ–æ–Ω–µ
        loop = asyncio.get_event_loop()
        loop.create_task(self.scheduled_questions())
        
        # –ó–∞–ø—É—Å–∫–∞–µ–º –±–æ—Ç–∞
        executor.start_polling(self.dp, skip_updates=True)

